<div class="div-estandar" id="divRegistrar">
    <h1 class="mb-4">Cambio de contraseña</h1>
    <div class="row div-registrar">
        <div class="mt-1 col-md-3">
            <label for="txtNombreRed" class="form-label">Nombre de red</label>
            <input type="text" class="form-control" id="txtNombreRed" placeholder="Nombre de red">
        </div>
        <div class="mt-1 col-md-3">
            <label for="txtPasswordRed" class="form-label">Contraseña de red</label>
            <input type="password" class="form-control" id="txtPasswordRed" placeholder="Contraseña de red">
        </div>
        <div class="mt-4 col-md-12 text-end">
            <button class="btn btn-primary col-2" id="btnGuardarCambio">Guardar</button>
        </div>
    </div>
</div>